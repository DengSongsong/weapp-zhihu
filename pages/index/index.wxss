/**index.wxss**/
.flex{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.top{
  width: 100%;
  height: 150rpx;
  position: fixed;
  top: 0;
  /*z-index: 9999;*/
  /*background-color: yellow;*/
}
.top .top-hd{
  width: 100%;
  height: 75rpx;
  margin-top: 10rpx;
  border-bottom: 2rpx solid #EBEBEB;
  /*background: white;*/
  background-color: white;
}
.top .search{
  width: 580rpx;
  height: 60rpx;
  background: white;
  /*background-color: darkblue;*/
}
.top .search input{
  display: inline-block;
  width: 100%;
  height: 60rpx;
  border-radius: 10rpx;
  background-color: #EBEBEB;
}
.top .search-placeholder{
  text-align: center;
  font-size: 30rpx;
}
.top .question{
  width: 120rpx;
  height: 60rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  /*background-color: red;*/
}
.top .question-icon{
  display: inline-block;
  height: 60rpx;
  line-height: 60rpx;
  
}
.iconfont.icon-tiwen{
  color: #0068C4;
  font-weight: bold;
}
.top .question-title{
  display: inline-block;
  height: 60rpx; 
  line-height: 60rpx;
  font-size: 30rpx;
  font-weight: bold;
  color: #0068C4;
}

@font-face {font-family: "iconfont";
  src: url('../fonts/iconfont.eot?t=1511409605749'); /* IE9*/
  src: url('../fonts/iconfont.eot?t=1511409605749#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
  url('../fonts/iconfont.ttf?t=1511409605749') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
  url('../fonts/iconfont.svg?t=1511409605749#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-tiwen:before { content: "\e60f"; }

.icon-fangdajing:before { content: "\e64f"; }

.top .top-bd{
  width: 100%;
  height: 74rpx;
  background: white;
  /*background-color: blue;*/
}
.top .swiper-tab{
  width: 100%;
  height: 74rpx;
  line-height: 74rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  color: #B4B4B4;
}
.swiper-tab .swiper-tab-item{
  font-size: 30rpx;
  font-weight: bold;
  flex: 1;
  text-align: center;
 
}
.active{
  color: #000;
  border-bottom: 2px solid #000;
}
.scroll-view{
  margin-top: 160rpx;
  
}
